<%= render partial: 'shared/header', :locals => { :active => 'Home' } %>
<div class="main-container bg-white">
  <section class="space-md">
    <div class="bg-white">
      <div class="container">
        <div class="row">
          <div class="col-12">
            <% if notice  %>
              <div class="alert alert-warning text-small" role="alert">
                <i class="icon-shield"></i>
                <span>
                    <%= notice %>
                </span>
              </div>
            <% end %>
                    
            <div class="text-left mb-2">
              <h4 class="h4 mb-2"><i class="icon-link mr-1"></i><%= @page.url %>
                <% if !@test.is_test_over %>
                  <span class="badge badge-success">Test Ongoing</span>
                <% else %>
                  <span class="badge badge-danger">Test Over</span>
                <% end %>
              </h4>
            </div>
            <div class="alert alert-info mt-4 mb-0" role="alert">
              <i class="icon-flash"></i>
              <span>
                  Google usually indexes a test within 1-3 days, so we will allow 3 days before we consider a test to be started.
              </span>
              <div class="row mt-2">
                <div class="col-12 col-lg-9">
                  <span>
                    <div class=""><i class="icon-calendar mr-1 "></i>Test Started: <%= fix_time(@test.starting_date) %></div>
                    <span class=""><i class="icon-calendar mr-1"></i>Ending on: <%= fix_time(@test.ending_date) %></span> 
                  </span>
                </div>
              </div>
            </div>
            
          </div>
        </div>
        
        <!--end of row-->
      </div>
      <!--end of container-->
    </div>
    
    
    

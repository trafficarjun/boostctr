                <h4 class="mb-0" style="color: #1a0dab;"><%= @test.previous_title %></h4>
                <span class="mb-0 mt-0" style="color: #006621;"><%= @test.page.url %></span>
                <p class="mt-0" style="color: #545454;">
                  <%= @test.previous_description %>
                </p>
              </div>
            </div>
          </div>
        </div>
        <!--end of col-->
